{
  "name": "dc-clone-ama-turborepoyla-yeter-artik",
  "devDependencies": {
    "prettier": "^3.5.0",
    "turbo": "^2.4.2",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.1.15",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "trustedDependencies": [
    "@clerk/shared",
    "core-js-pure",
    "discord-clone"
  ],
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "appwrite": "^17.0.0"
  }
}
